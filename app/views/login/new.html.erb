<%= link_to "Sign up", new_user_path %>
<br><br>
<%= form_tag login_index_path do %>
      <%= label_tag :username, "Your username:" %>
      <%= text_field_tag :username %> <br>
      <%= label_tag :password, "Your password:"%>
      <%= password_field_tag :password %><br><br>
  <%= submit_tag "Log in!" %>
<% end %>
