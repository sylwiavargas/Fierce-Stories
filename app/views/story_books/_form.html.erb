<%= link_to "Story Book Index Page", story_books_path %> <br>
<br><br>

<%= form_for @story_book do |f| %>

t.string "tag"
t.string "comment"
t.integer "user_id"
t.integer "story_id"

<%= f.label :tag %>
<%= f.text_field :tag %> <br>

<%= f.label :comment %>
<%= f.text_field :comment %><br>

<%= f.label :story_id %>
<%= f.collection_select :pronoun_one_id, PronounOne.order(:name).all, :id, :name, :prompt => '~please select~' %><br>

<%= f.label :pronoun_two_id %>
<%= f.collection_select :story_id, Story.order(:name).all, :id, :name, :prompt => '~please select~' %><br>

<%= f.submit %><br>

<% end %>
